{
	
"name": "CAPTCHA Security Inspector",
	
"description": "a tool for CAPTCHA devs for testing their CAPTCHAs",
	
"version": "1.3.2",
	
"manifest_version": 3,

"key": "",
	
"background": {"service_worker": "background.js"},

"content_scripts": 
[{
  "matches": ["https://*/*"],
  "js": ["content.js"]
}],

"permissions": ["scripting", "activeTab"],

"host_permissions": ["https://*/*", "http://*/*"],

"web_accessible_resources": [{"resources": ["images/solve_image.png", "icons/icon32.png", "audio/tada.mp3"], "matches": ["https://*/*"]}],

"icons": 
{
  "16": "icons/icon16.png",
  "32": "icons/icon32.png",
  "48": "icons/icon48.png",
  "128": "icons/icon128.png"
}
	
}